generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Link {
  id        String    @id @default(cuid())
  createdAt DateTime  @default(now())
  deletedAt DateTime?
  slug      String    @unique
  url       String
  viewCount Int       @default(0)
}
