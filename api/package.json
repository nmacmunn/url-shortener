{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "appmap": "appmap-agent-js -- node -r dotenv/config build/index.js",
    "build": "tsc -p ./",
    "dev": "nodemon -r dotenv/config src/index.ts",
    "migrate:dev": "prisma migrate dev",
    "migrate:test": "dotenv -v DATABASE_URL=file:../../data/test.db -- prisma migrate dev",
    "preview": "node -r dotenv/config build/index.js",
    "test": "dotenv -v DATABASE_URL=file:../../data/test.db -- jest",
    "test:watch": "dotenv -v DATABASE_URL=file:../../data/test.db -- jest --watchAll"
  },
  "dependencies": {
    "@prisma/client": "4.13.0",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "express": "4.18.2"
  },
  "devDependencies": {
    "@appland/appmap-agent-js": "^13.8.0",
    "@types/cors": "^2.8.13",
    "@types/express": "4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "18.16.2",
    "@types/supertest": "^2.0.12",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prisma": "4.13.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
